pipeline{
    agent any
    stages{
        stage('1'){
            steps('c'){
                sh '''
                    case "$language" in
                    "c") sh 'C'
                    ;;
                    "JAVA") sh 'JAVA'
                    ;;
                    "PYTHON") sh 'PYTHON'
                    ;;
                    "all") echo "all"
                    ;;
                    esac
                '''
            }
        }
    }
}
