pipeline{
    agent any
    stages{
        stage('1'){
            steps('c'){
                sh '''
                    case "$1" in
                    "c") 
                    echo 'C'
                    ;;
                    "java") 
                    echo 'JAVA'
                    ;;
                    "python") 
                    echo 'PYTHON'
                    ;;
                    "all") 
                    echo "all"
                    ;;
                    esac
                '''
            }
        }
    }
}
