pipeline{
    agent any
    stages{
        stage('1'){
            steps('c'){
                sh '''
                    case "$language" in
                    "c") echo 'C'
                    ;;
                    "java") echo 'JAVA'
                    ;;
                    "python") echo 'PYTHON'
                    ;;
                    "all") echo "all"
                    ;;
                    esac
                '''
            }
        }
    }
}
