pipeline{
    agent any
    stages{
        stage('1'){
            steps('c'){ 
                sh '''
                    case "${language}" in
                    "c") sh C
                    ;;
                    "java") sh JAVA
                    ;;
                    "python") sh ' echo "PYTHON" '
                    ;;
                    "all") echo "all" 
                    ;;
                    esac
                '''
            }
        }
    }
}
